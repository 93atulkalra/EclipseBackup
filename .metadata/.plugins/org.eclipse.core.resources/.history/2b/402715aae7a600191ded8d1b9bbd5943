package Interviewquestion_Codingprac;

import java.util.Collections;
import java.util.LinkedList;

public class NamePrefixCheck {
public static void main(String[] args) {
	
	LinkedList<String> arr = new LinkedList<String>() ;
	arr.add("John");
	arr.add("Tom");
	arr.add("Alas");
	arr.add("John");
	
	LinkedList<String> opt = new LinkedList<String>() ;
	
	int i,j,count;
	int prefix =1;
	
	for (i=0;i<arr.size();i++)
	{
		count=0;prefix=0;
		if(Collections.frequency(arr, arr.get(i))>1&&opt.contains(arr.get(i)))
				{
			
		for(j=0;j<arr.size();j++)
			
		{
			if ( arr.get(i)==arr.get(j))
				{
				count++;
			
			if(count > 1)
			{
				 arr.get(i).concat(String.valueOf(prefix));
				 prefix++;
				 System.out.println(arr.get(i));
				 
			}
				}// if outer ends 	
			
		}  // J ends here
				}  // If ends here
		
		System.out.println("ends");
		opt.add(arr.get(i));
		System.out.println(arr.get(i));
	} // i ends here
	
	System.out.println(opt);
	
	
}
}
